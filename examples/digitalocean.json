{
	"blueprint": "digitalocean {*user?} {~server?}",
	"command": "ssh {user}@{server}",
	"help": "Connects to ssh server using provided user",

	"arguments": {
		"user": {
			"values": {
				"default": "root"
			},
			"help": "User name"
		},
		"server": {
			"values": {
				"default": "123.456.78.910",

				"website": "123.456.78.910",
				"git": "91.876.54.321"
			},
			"help": "Server name",
			"errors": {
				"invalid": "I've never heard of {server}."
			}
		}
	}
}
